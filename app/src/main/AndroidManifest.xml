<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/bos_akvaryum_icon"
        android:label="Balikci Hasan"
        android:roundIcon="@drawable/bos_akvaryum_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Forestapp"
        tools:targetApi="31">

        <!-- Ana Aktivite (Uygulamanın başlangıç noktası) -->
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="Balikci Hasan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".RozetActivity"
            android:exported="true" />
        <activity
            android:name=".ShopActivity"
            android:exported="true" />

    </application>
</manifest>